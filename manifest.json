{
    "manifest_version": 2,
    "name": "人力銀行應徵人數顯示",
    "description": "提供人力銀行職缺應徵者人數反推計算",
    "version": "1.0.2",
    "author": "Pei-Yao Xu",
    "content_scripts": [
        {
            "matches":[
                "*://www.1111.com.tw/*",
                "*://www.104.com.tw/*"
            ],
            "js":[
                "Http.js",
                "Main.js"
            ]
        },
        {
            "matches": [
                "*://www.1111.com.tw/*"
            ],
            "js": [
                "banks/Bank_1111.js"
            ]
        },
        {
            "matches": [
                "*://www.1111.com.tw/job-bank/job-index.asp?*"
            ],
            "js": [
                "StartupList.js"
            ]
        },
        {
            "matches": [
                "*://www.1111.com.tw/corp/*"
            ],
            "js": [
                "StartupList.js"
            ]
        },
        {
            "matches": [
                "*://www.1111.com.tw/job/*"
            ],
            "js": [
                "StartupPage.js"
            ]
        },
        {
            "matches": [
                "*://www.104.com.tw/*"
            ],
            "js": [
                "banks/Bank_104.js"
            ]
        },
        {
            "matches": [
                "*://www.104.com.tw/jobbank/joblist/joblist.cfm?*"
            ],
            "js": [
                "StartupList.js"
            ]
        },
        {
            "matches": [
                "*://www.104.com.tw/jobbank/custjob/index.php?*"
            ],
            "js": [
                "StartupList.js"
            ]
        },
        {
            "matches": [
                "*://www.104.com.tw/job/?*"
            ],
            "js": [
                "StartupPage.js"
            ]
        }
    ],
    "permissions": []
}